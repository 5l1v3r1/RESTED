{
  "name": "rested",
  "title": "RESTED",
  "id": "rested@restedclient",
  "homepage": "https://github.com/esphen/RESTED",
  "description": "A REST client for the rest of us",
  "author": "Espen Henriksen",
  "license": "MIT",
  "version": "1.0.0",
  "main": "./main.js",
  "permissions": {
    "multiprocess": true
  },
  "scripts": {
    "test": "jest --config test/jest.conf.js",
    "test:watch": "jest --watch --config test/jest.conf.js",
    "test:snapshot": "jest -u --config test/jest.conf.js",
    "eslint": "eslint src",
    "start": "NODE_ENV=dev rollup -w --config dev/rollup/rollup.config.js",
    "build": "NODE_ENV=production rollup --config dev/rollup/rollup.config.js"
  },
  "dependencies": {
    "aphrodite": "^0.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^16.0.1",
    "react-test-renderer": "^15.3.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^2.5.0"
  }
}
