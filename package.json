{
  "name": "rested",
  "title": "RESTED",
  "id": "rested@restedclient",
  "homepage": "https://github.com/esphen/RESTED",
  "description": "A REST client for the rest of us",
  "author": "Espen Henriksen",
  "license": "MIT",
  "version": "1.0.0",
  "main": "./main.js",
  "permissions": {
    "multiprocess": true
  },
  "scripts": {
    "test": "jest --coverage --config test/jest.conf.js",
    "test:watch": "jest --watchman --watchAll --config test/jest.conf.js",
    "test:snapshot": "jest --update-snapshot --config test/jest.conf.js",
    "test:debugger": "node --debug-brk --inspect ./node_modules/.bin/jest -i --config test/jest.conf.js",
    "eslint": "eslint src test",
    "eslint:fix": "eslint --fix src test",
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack --progress --colors --watch --config dev/webpack/webpack.config.js",
    "build": "NODE_ENV=production webpack --progress --config dev/webpack/webpack.config.js"
  },
  "dependencies": {
    "aphrodite": "^1.0.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "lodash.flow": "^3.5.0",
    "object-assign": "^4.1.0",
    "react": "15.4.0-rc.4",
    "react-bootstrap": "^0.30.5",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "15.4.0-rc.4",
    "react-highlight": "^0.9.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-form": "^6.1.0",
    "redux-thunk": "^2.1.0",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^16.0.1",
    "react-addons-test-utils": "15.4.0-rc.4",
    "react-dnd-test-backend": "^1.0.2",
    "react-test-renderer": "15.4.0-rc.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "webpack": "2.1.0-beta.25"
  }
}
